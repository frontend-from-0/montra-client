{
  "extends": ["react-app",
  "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", {
      "semi": true,
      "singleQuote": true,
      "jsxSingleQuote": true,
      "trailingComma": "all",
      "printWidth": 80,
      "tabWidth": 2
    }]
  }
}